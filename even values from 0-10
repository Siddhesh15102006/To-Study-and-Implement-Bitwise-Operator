// print even values between 0 Add 10
#include <iostream>
using namespace std;
int main() 
{
    cout << "even number between 0 and 10:"<<endl;
    for (int i = 0; i <= 10; ++i)
    {
        if (i % 2 == 0)
    {
        cout<<i<<"";
    }
    }
    return 0;
}

/*Output
even number between 0 and 10:
0246810

=== Code Execution Successful ===
//
